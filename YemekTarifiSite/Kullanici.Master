<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Kullanici.master.cs" Inherits="YemekTarifiSite.Kullanici" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style type="text/css">
        .auto-style1 {
            width: 100%;
        }

        .auto-style3 {
            text-align: center;
            width: 145px;
            height: 39px;
        }

        .auto-style5 {
            text-align: center;
            height: 39px;
        }

        .auto-style6 {
            text-align: left;
        }

        .auto-style7 {
